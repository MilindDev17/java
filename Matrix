import java.util.Scanner;
import java.util.*;

public class gbus {
	public static void main(String args[]) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[][] x = new int[N][2];
		for(int i = 0;i<x.length;i++){
			for(int j = 0;j<x.length;j++){
			System.out.print(x[i][j]);
			}
			
			System.out.println();	
		}
		
	}
}
