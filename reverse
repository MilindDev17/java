import java.util.*;
import java.util.Scanner;

public class middlerever {
	public static void main(String args[]) {
		Scanner sc =  new Scanner(System.in);
		int x = sc.nextInt();
		int mid = x/2;
		int[] arr = new int[x];
	
		for (int i = 0 ; i < arr.length; i++) {
				arr[i] = sc.nextInt();
		} 
		for(int i = arr.length - mid - 1;i > 0;i--) {
			System.out.println(arr[i]);
			
		}
	}
}
