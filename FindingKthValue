package dataval;

import java.util.Scanner;

/**
 *
 * @author MILIND_Dev
 */
public class FindingK {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str = sc.next();
        int n = sc.nextInt();
        String[] st = new String[n];
        for (int i = 0; i < str.length(); i++) {
            for (int j = 2; j < str.length(); j++) {
                if(st[i] != st[j]) {
                    i++;
                }
            }
        }
    }
}
